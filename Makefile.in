#########################################
# Object files
#########################################
$(OBJS): src/*.h Makefile configure

#########################################
# install
#########################################
install:
	mkdir -p $(DESTDIR)/include
	$(INSTALL) -m 644 ./comelang.h $(DESTDIR)/include
	mkdir -p $(DESTDIR)/lib
	if test -e ./libcomelang.a; then $(INSTALL) -m 644 ./libcomelang.a $(DESTDIR)/lib; fi
	if test -e ./libcomelang-gc.a; then $(INSTALL) -m 644 ./libcomelang-gc.a $(DESTDIR)/lib; fi
	if test -e ./libcomelang-ts.a; then $(INSTALL) -m 644 ./libcomelang-ts.a $(DESTDIR)/lib; fi

	mkdir -p "$(DESTDIR)/bin"
	$(INSTALL) -m 755 ./comelang "$(DESTDIR)/bin"

	mkdir -p $(DESTDIR)/share/doc/comelang
	$(INSTALL) -m 644 ./README.md "$(DESTDIR)/share/doc/comelang"

#########################################
# permission
#########################################
permission:
	chmod 644 *
	chmod 755 .git man src configure
	chmod 644 src/*.c
	chmod 644 src/*.h
	chmod 755 update_comelang.sh

#########################################
# clean
#########################################
clean:
	rm -fR a d c.c.c d.c.c comelang comelang2 *.a src/*.o src/*.i src/*.ll code/*.i code/*.ll code/HelloWorld code/HelloWorld2 code/HelloWorld3 code/HelloWorld4 code/HelloWorld5 *.i *.ll a.out src/*.o code/HelloWorld.dSYM code/HelloWorld2.dSYM code/HelloWorld3.dSYM code/HelloWorld4.dSYM code/HelloWorld5.dSYM code/HelloWorld6.dSYM code/HelloWorld7.dSYM code/HelloWorld8.dSYM code/HelloWorld9.dSYM code/HelloWorld10.dSYM code/HelloWorld11.dSYM code/HelloWorld12.dSYM code/HelloWorld13.dSYM code/HelloWorld14.dSYM code/HelloWorld15.dSYM code/HelloWorld16.dSYM code/HelloWorld17.dSYM comelang.dSYM code/HelloWorld6 code/HelloWorld7 code/HelloWorld8 code/HelloWorld9 code/HelloWorld10 code/HelloWorld11 code/HelloWorld12 code/HelloWorld13 code/HelloWorld14 code/HelloWorld15 code/HelloWorld16 code/HelloWorld17 code/HelloWorld18 code/HelloWorld19 code/HelloWorld20 code/HelloWorld*-ts b c a *.o libcomelang.a a *.i a.out code/tcctest.c.o *.c.c code/HelloWorld*.c.c HelloWorld*


