#########################################
# installed directories
#########################################
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
datadir=@datadir@
mandir=@mandir@
sharedstatedir=@sharedstatedir@
sysconfdir=@sysconfdir@/comelang
includedir=@includedir@/comelang
datarootdir=@datarootdir@/comelang
docdir=@datadir@/doc
libdir=@libdir@

#########################################
# environmnet variables
#########################################
CC=@CC@
CXX=@CXX@
INSTALL=@INSTALL@
CFLAGS=@CFLAGS@
CFLAGS2=`echo $(CFLAGS) | sed -e 's/-O2//'`
CXXFLAGS=@CXXFLAGS@
CXXFLAGS2=@CXXFLAGS2@
LIBS=@LIBS@
OS=@OS@
DESTDIR=@DESTDIR@
OBJS=@OBJS@
CLANG=@CLANG@
SELF_HOST_OBJS=@SELF_HOST_OBJS@

#########################################
# main
#########################################
all: comelang no-self-host 

no-self-host: comelang lib lib-gc #lib-ts

comelang2-install: 
	$(INSTALL) -m 755 ./comelang2 "$(DESTDIR)/bin"
	mkdir -p $(DESTDIR)/include
	$(INSTALL) -m 644 ./comelang2.h $(DESTDIR)/include

comelang2: common.h 01main.o 02transpile.o 03transpile2.o 04heap.o 05function.o 06str.o 07var.o 08if.o 09while.o 10do_while.o 11for.o 12switch.o 13op.o 14struct.o 15union.o 16enum.o 17typedef.o 18field.o 19eq.o 20method.o 21obj.o 22impl.o
	./comelang -no-gc -g -o comelang2 01main.o 02transpile.o 03transpile2.o 04heap.o 05function.o 06str.o 07var.o 08if.o 09while.o 10do_while.o 11for.o 12switch.o 13op.o 14struct.o 15union.o 16enum.o 17typedef.o 18field.o 19eq.o 20method.o 21obj.o 22impl.o
	mkdir -p "$(DESTDIR)/bin"

01main.o: 01main.c common.h
	./comelang -g -no-gc -c 01main.c

02transpile.o: 02transpile.c common.h
	./comelang -no-gc -g -c 02transpile.c

03transpile2.o: 03transpile2.c common.h
	./comelang -no-gc -g -c 03transpile2.c

04heap.o: 04heap.c common.h
	./comelang -no-gc -g -c 04heap.c

05function.o: 05function.c common.h
	./comelang -no-gc -g  -c 05function.c

06str.o: 06str.c common.h
	./comelang -no-gc -g -c 06str.c

07var.o: 07var.c common.h
